{
    "name": "vscode-wisl",
    "displayName": "vscode-wisl",
    "description": "Language support for WISL (While In Separation Logic)",
    "version": "0.2.0",
    "publisher": "giltho",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "wisl",
                "aliases": [
                    "WISL",
                    "wisl"
                ],
                "extensions": [
                    ".wisl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "wisl",
                "scopeName": "source.wisl",
                "path": "./syntaxes/wisl.tmLanguage.json"
            }
        ]
    }
}
